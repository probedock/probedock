angular.module('probedock').run(function($templateCache) {
  var templatesVersion = 3;
  var version = '0.1.25';

  <% if Rails.env == 'production' %>
  <% require 'templates' %>
  <% environment.context_class.instance_eval { include ApplicationTemplateUtils } %>

  <% generate_template_scripts.each do |template| %>
  $templateCache.put(<%= template[:id].to_json %>, <%= template[:contents].to_json %>);
  <% end %>
  <% end %>
});
